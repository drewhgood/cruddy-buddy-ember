<div class="wrapper wrapper-content animated fadeInRight">
        

        <div class="row">
{{#each emp in model.employee}}
     
            <div class="col-lg-4">
                <div class="contact-box">
                    <div class="col-sm-4">
                        <div class="text-center">
                            <img alt="image" class="img-circle m-t-xs img-responsive" src="img/a2.jpg">
                            <div class="m-t-xs font-bold">Graphics designer</div>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <h3><strong>{{emp.user.first_name}}</strong></h3>
                        <p><i class="fa fa-map-marker"></i> Riviera State 32/106</p>
                        <address>
                            <strong>{{emp.user.address}}</strong><br>
                            
                            <abbr title="Phone">P:</abbr> {{emp.user.phone}}
                        </address>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
 
{{/each}}
            
        </div>

</div>